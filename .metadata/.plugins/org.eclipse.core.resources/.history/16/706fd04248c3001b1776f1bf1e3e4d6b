package day03;

public class WhileTest3 {
	
	public static void main(String[] args) {
		
		// 5부터 10까지
		int i = 5;
		while(i<=10) {
			
			System.out.print(i+" ");
			i++;
		}
			System.out.println();
		
		
		// 10부터 5까지
		i=10;
		while(i>=5) {
			System.out.print(i+" ");
			i--;
		}
		System.out.println();
		
		//시작값과 끝값을 입력받아 그 구간을 출력.
		//시작값 3 끝값 10
		i=3;
		while(i<=10) {
			System.out.print(i+" ");
			i++;
		}
		System.out.println();
	}

}
